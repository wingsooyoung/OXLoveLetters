<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ title }}</title>
    <style>
      {% include "css/styles.css" ignore missing %}
    </style>
  </head>
  <body class="{{ pageClass }}">
    <div class="container">
    <header>
      <ul class="nav">
        <li>
          <a href="/">home<small>where the links are</small></a>
        </li>
        <li>
          <a href="/about">about<small>where the info is</small></a>
        </li>
      </ul>
    </header>
    {{ content | safe }}
    {% include "footer.njk" %}
    </div>
    {% set js %}
      {% include "js/validate.js" %}
    {% endset %}
    <script>
      {{ js | jsmin | safe }}
    </script>
  </body>
</html>



